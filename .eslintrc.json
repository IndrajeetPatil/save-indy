{
  "env": {
    "browser": true,
    "es2022": true
  },
  "extends": "",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "double"],
    "max-len": ["error", { "code": 120 }],
    "require-jsdoc": "off"
  },
  "editor.codeActionsOnSave": { "source.fixAll.eslint": true }
}
